<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="1000" />
    <simulation finalTime="40.0" >
        <type>turbulence_kw</type>
            <bl_type>non-existing</bl_type>
        <scenario>channel</scenario>
    </simulation>
    <timestep dt="1" tau="0.5" />
    <solver gamma="0.5" />
    <geometry
      dim="2"
      lengthX="30.0" lengthY="1.0" lengthZ="1.0"
      sizeX="60" sizeY="60" sizeZ="10"
      stretchX="false" stretchY="true" stretchZ="true"
    >
      <mesh>uniform</mesh>
      <!--mesh>stretched</mesh-->
    </geometry>
    <environment gx="0" gy="0" gz="0" />
    <walls>
        <left>
            <vector x="1.0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="0.0" y="0." z="0." />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="0.1">Channel2D_KW</vtk>
    <stdOut interval="0.0001" />
    <parallel numProcessorsX="1" numProcessorsY="1" numProcessorsZ="1" />
</configuration>
